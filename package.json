        "@parcel/transformer-elm": "^2.6.0",
        "parcel": "^2.6.0",
        "@types/node": "^17.0.23",
        "@types/ws": "^8.5.3",
        "elm": "^0.19.1-5",
        "gh-pages": "^3.2.3",
        "serve": "^13.0.2"
    }
}
